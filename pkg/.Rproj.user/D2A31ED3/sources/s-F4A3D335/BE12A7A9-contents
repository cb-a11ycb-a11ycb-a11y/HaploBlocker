# Genomic Relationship matrices

library(miraculix)

G_chip <- relationshipMatrix(codeGeno(geno_chip))
G_blocker <- relationshipMatrix(codeGeno(geno_blocker))
G_window <- relationshipMatrix(codeGeno(geno_window))
G_window2 <- relationshipMatrix(codeGeno(geno_window2))
G_code <- relationshipMatrix(codeGeno(geno_code))
G_code2 <- relationshipMatrix(codeGeno(geno_code2))
G_code_blocker <- relationshipMatrix(codeGeno(geno_code_blocker))

save(file="NRGene_MAZE_sequence/Datasets_PE340_Gmatrix.RData", list=c("G_chip", "G_code_blocker","G_blocker", "G_window", "G_window2", "G_code", "G_code2"))


